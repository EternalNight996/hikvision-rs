{
  "package": {
    "productName": "heg-record-camera",
    "version": "0.1.0"
  },
  "window": {
    "theme": "Mask",
    "ui": {
      "title": "#name#-#version#",
      "className": "#name#",
      "x": 0,
      "y": 0,
      "width": 1024,
      "height": 600
    }
  },
  "log": {
    "level": "Debug",
    "folder": "{origin}/#name#-#version#/logs/{date}",
    "fname": "#name#.log",
    "format": "{level}{target}{file}{line}{thread-ids}{thread-names}",
    "outputList": [
      "folder",
      "stdout",
      "webview"
    ]
  },
  "camera": {
    "ui": {
      "title": "机内检查-#version#",
      "className": "#name#-mii",
      "x": 0,
      "y": 0,
      "width": 400,
      "height": 350
    },
    "loginInfo": {
      "host": "10.10.6.23",
      "port": 8000,
      "uname": "admin",
      "passwd": "admin123",
      "protocal": "Hikvision"
    },
    "logDir": "{origin}/#name#-#version#/hik-logs/{date}",
    "cacheDir": "{origin}/#name#-#version#/cache/{date}",
    "autoCapture": true,
    "apiType": "HikMvs",
    "mvs": {
      "quality": 60,
      "imageType": 2,
      "methodValue": 0,
      "mSec": 3000
    },
    "net": {
      "bufSize": 10485760,
      "maxCache": 100
    }
  }
}